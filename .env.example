# Claude API Key (used by Ring 1 evolution engine)
CLAUDE_API_KEY=

# DashScope API Key (for Qwen / 千问 models)
DASHSCOPE_API_KEY=

# Per-machine LLM provider override (takes precedence over config.toml)
# Uncomment to use Qwen instead of the default Anthropic:
# LLM_PROVIDER=qwen
# LLM_API_KEY_ENV=DASHSCOPE_API_KEY
# LLM_MODEL=qwen3.5-plus
# LLM_MAX_TOKENS=8192

# Telegram Bot Token
TELEGRAM_BOT_TOKEN=

# Telegram Chat ID (auto-detected from first message if empty)
TELEGRAM_CHAT_ID=
